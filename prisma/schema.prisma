
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

// TworzÄ™ model user do bazy postgreSQL
model User {
  id        Int    @id @default(autoincrement())
  name      String
  vorname   String?

  userLogin UserLogin?
}

model UserLogin {
  id       Int    @id @default(autoincrement())
  password String
  email    String @unique
  userId   Int   @unique

  user     User    @relation(fields: [userId], references: [id])

}